                <asp:View ID="loginPage" runat="server">

                    <div class="container">
                        <div>
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-lg-3"></div>
                                    <div class="col-lg-5">

                                        <div class="signUpFormHighLight" style="opacity: 0.8">
                                            <div class="page-header">
                                                <h2>Login Page</h2>
                                            </div>

                                            <div class="input-group" style="width: 240px;">
                                                <asp:TextBox class="form-control" placeholder="UserName" aria-describedby="basic-addon1" runat="server" ID="loginUserName"></asp:TextBox>

                                            </div>
                                            <div class="input-group" style="width: 240px; padding-top: 0.5em;">
                                                <asp:TextBox class="form-control" TextMode="Password" placeholder="Password" aria-describedby="basic-addon1" runat="server" ID="loginPassword"></asp:TextBox>

                                            </div>
                                            <div class="input-group">
                                                <div class="input-group" style="float: left; padding-top: 0.5em;">
                                                    <asp:CheckBox ID="CheckBox1" runat="server" />

                                                </div>
                                                <div class="input-group" style="float: right; padding-top: 0.5em;">
                                                    Keep me logged in
                                                </div>
                                            </div>
                                            <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                                                <div class="btn-group" role="group" aria-label="..." style="text-align: center;">
