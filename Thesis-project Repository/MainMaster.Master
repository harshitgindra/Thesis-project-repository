                <asp:View ID="signUpPage" runat="server">

                    <div class="container">

                        <div class="container" style="position: relative">

                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-lg-3"></div>
                                    <div class="col-lg-5">
                                        <div class="signUpFormHighLight" style="opacity: 0.85">
                                            <div class="page-header">
                                                <h2>Sign Up Form</h2>
