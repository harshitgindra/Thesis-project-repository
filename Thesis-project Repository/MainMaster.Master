                                                    </asp:RadioButtonList>
                                                </div>

                                               
                                                    <asp:RequiredFieldValidator ControlToValidate="retrievelMethodRadioList" ID="RequiredFieldValidator11" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>

                                                <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                                                    <asp:Button ID="Button3" class="btn btn-default" runat="server" Text="Retrieve Password" OnClick="RetrieveForgotPassword" />
                                                </div>
                                                <asp:Label ID="confirationMessage" ForeColor="Green" runat="server"></asp:Label>
