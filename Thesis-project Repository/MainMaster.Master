                                                <div class="input-group" style="width: 240px; padding-top: 0.5em;">

                                                    <asp:RadioButtonList ID="FpAccntType" runat="server" RepeatDirection="Horizontal">
                                                        <asp:ListItem Text="Student" Value="S" />
