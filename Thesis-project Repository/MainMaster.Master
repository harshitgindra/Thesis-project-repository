                                                        <asp:ListItem Text="Viewer" Value="V" />
                                                    </asp:RadioButtonList>
                                                     
                                                    <asp:RequiredFieldValidator ControlToValidate="FpAccntType" ID="RequiredFieldValidator12" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                                <div class="input-group" style="width: 240px; padding-top: 0.5em;">
                                                    <asp:RadioButtonList ID="retrievelMethodRadioList" runat="server" RepeatDirection="Horizontal">
                                                        <asp:ListItem Text="Email" Value="E" />
