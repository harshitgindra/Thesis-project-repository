                                                <asp:ScriptManager ID="ScriptManager1" runat="server">
                                                </asp:ScriptManager>
                                                <asp:UpdatePanel ID="UpdatePanel1" runat="server" ChildrenAsTriggers="True" ViewStateMode="Enabled">
                                                    <ContentTemplate>
                                                        <asp:Label ID="usernameerror" runat="server" Text=""></asp:Label>
                                                    </ContentTemplate>
                                                    <Triggers>
                                                        <asp:AsyncPostBackTrigger ControlID="signUpUsername" />
                                                    </Triggers>
                                                </asp:UpdatePanel>
                                            </div>
                                            <div class="input-group">
                                                <div class="input-group" style="float: left; width: 187px; padding-top: 0.3em; padding-right: 0.5em;">
                                                    <asp:TextBox class="form-control" placeholder="Password" TextMode="Password" aria-describedby="basic-addon1" runat="server" ID="signUpPassword">
                                                    </asp:TextBox><asp:RequiredFieldValidator ControlToValidate="signUpPassword" ID="RequiredFieldValidator2" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                                <div class="input-group " style="float: right; width: 187px; padding-top: 0.3em;">
                                                    <asp:TextBox class="form-control" placeholder="Confirm Password" aria-describedby="basic-addon1" TextMode="Password" runat="server" ID="signUpPassword2"></asp:TextBox>
                                                    <asp:CompareValidator ControlToValidate="signUpPassword2" ControlToCompare="signUpPassword" ID="CompareValidator1" runat="server" ErrorMessage="Passwords do not match"></asp:CompareValidator>
                                                </div>
                                            </div>
                                            <div class="input-group">
                                                <div class="input-group" style="float: left; width: 187px; padding-top: 0.3em; padding-right: 0.5em;">
                                                    <asp:TextBox class="form-control" placeholder="First Name" aria-describedby="basic-addon1" runat="server" ID="fname">
                                                    </asp:TextBox>
                                                    <asp:RequiredFieldValidator ControlToValidate="fname" ID="RequiredFieldValidator3" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                                <div class="input-group" style="float: right; width: 187px; padding-top: 0.3em;">
                                                    <asp:TextBox class="form-control" placeholder="Last Name" aria-describedby="basic-addon1" runat="server" ID="lname"></asp:TextBox>
                                                    <asp:RequiredFieldValidator ID="RequiredFieldValidator4" runat="server" ControlToValidate="lname" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                            </div>
                                            <div class="input-group">
                                                <div class="input-group" style="float: left; width: 187px; padding-top: 0.3em; padding-right: 0.5em;">
                                                    <asp:TextBox class="form-control" placeholder="Security Question" aria-describedby="basic-addon1" runat="server" ID="secques">
                                                    </asp:TextBox>
                                                    <asp:RequiredFieldValidator ControlToValidate="secques" ID="RequiredFieldValidator5" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                                <div class="input-group" style="float: right; width: 187px; padding-top: 0.3em;">
                                                    <asp:TextBox class="form-control" placeholder="Security Answer" aria-describedby="basic-addon1" runat="server" ID="secans"></asp:TextBox>
                                                    <asp:RequiredFieldValidator ControlToValidate="secans" ID="RequiredFieldValidator6" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                            </div>
                                            <div class="input-group">
                                                <div class="input-group" style="float: left; width: 187px; padding-top: 0.3em; padding-right: 0.5em;">
                                                    <asp:TextBox class="form-control" placeholder="Phone Number" aria-describedby="basic-addon1" runat="server" ID="phoneNumber">
                                                    </asp:TextBox>
                                                    <asp:RequiredFieldValidator ControlToValidate="phoneNumber" ID="RequiredFieldValidator7" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                                <div class="dropdown" style="float: right; width: 187px; padding-top: 0.3em;">
                                                    <asp:DropDownList ID="ntwrkprovider" CssClass="form-control" runat="server">
                                                        <asp:ListItem Selected="True">Provider</asp:ListItem>
                                                        <asp:ListItem>boost mobile</asp:ListItem>
                                                        <asp:ListItem>t-mobile</asp:ListItem>
                                                        <asp:ListItem>virgin mobile</asp:ListItem>
                                                        <asp:ListItem>cingular</asp:ListItem>
                                                        <asp:ListItem>sprint nextel</asp:ListItem>
                                                        <asp:ListItem>verizon</asp:ListItem>
                                                        <asp:ListItem>nextel</asp:ListItem>
                                                        <asp:ListItem>us cellular</asp:ListItem>
                                                        <asp:ListItem>suncom</asp:ListItem>
                                                        <asp:ListItem>powertel</asp:ListItem>
                                                        <asp:ListItem>att cingular</asp:ListItem>
                                                        <asp:ListItem>alltel</asp:ListItem>
                                                        <asp:ListItem>metro pcs</asp:ListItem>
                                                        <asp:ListItem></asp:ListItem>
                                                    </asp:DropDownList>
                                                    <asp:RequiredFieldValidator ControlToValidate="ntwrkprovider" ID="RequiredFieldValidator8" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                                </div>
                                            </div>
                                            <div class="input-group" style="text-align: center; padding-top: 0.3em;">
                                                <asp:RadioButtonList ID="accType" runat="server" RepeatDirection="Horizontal">
                                                    <asp:ListItem Text="Student" Value="S" />
                                                    <asp:ListItem Text="Professor" Value="P" />
                                                    <asp:ListItem Text="Viewer" Value="V" />
                                                </asp:RadioButtonList>
                                                &nbsp;&nbsp;
                                <asp:RequiredFieldValidator ControlToValidate="accType" ID="RequiredFieldValidator9" runat="server" ErrorMessage="**"></asp:RequiredFieldValidator>
                                            </div>

                                            <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.3em;">
                                                <asp:Button Text="Submit" class="btn btn-default" ID="signUp" runat="server" OnClick="SignUp"></asp:Button>
                                            </div>
                                            <div>
