                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
               
                </asp:View>

                <asp:View ID="changePasswordMessage" runat="server">

                    <div class="container">

                        <div class="container-fluid">
                                <div class="row">
                                    <div class="col-lg-3"></div>
                                    <div class="col-lg-5">

                                        <div class="signUpFormHighLight" style="opacity: 0.9">
                                            <div class="page-header">
                                                <h2>Change Password</h2>
                                            </div>
                        <div class="input-group" style="width: 361px; padding-top: 0.5em;">
                            <asp:TextBox class="form-control" placeholder="Verification Code" EnableViewState="false" aria-describedby="basic-addon1" runat="server" ID="VerificationCode"></asp:TextBox>
                        </div>
                        <div class="input-group" style="width: 361px; padding-top: 0.5em;">
                            <asp:TextBox class="form-control" TextMode="Password" placeholder="New Password" EnableViewState="false" aria-describedby="basic-addon1" runat="server" ID="NewPassword"></asp:TextBox>
                        </div>
                        <div class="input-group" style="width: 361px; padding-top: 0.5em;">
                            <asp:TextBox class="form-control" TextMode="Password" placeholder="Confirm Password" EnableViewState="false" aria-describedby="basic-addon1" runat="server" ID="CnfrmPassword"></asp:TextBox>
                            <asp:CompareValidator ID="CompareValidator2" ControlToValidate="CnfrmPassword" ControlToCompare="NewPassword" runat="server" ErrorMessage="**"></asp:CompareValidator>
                        </div>
                        <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                            <asp:Button Text="Change Password" class="btn btn-default" ID="UpdatePass" runat="server" OnClick="UpdatePasswordUsingSms"></asp:Button>

                        </div>
                        <br />
                        <asp:Label ID="UpdatePasswordFromSMSConfirmation" runat="server" Text=""></asp:Label>

                    </div>
                                        </div>
                                    <div class="col-lg-3"></div>
                                    </div>
                            </div>
                        </div>
                   
