                            <asp:LinkButton ID="LinkButton2" runat="server" OnClick="SignUpLink" Text="Sign Up"><span class="glyphicon glyphicon-thumbs-up"></span>&nbsp;SignUp</asp:LinkButton></li>
                        <li>
                            <asp:LinkButton ID="LinkButton3" runat="server" OnClick="LoginLink" Text="Log In"><span class="glyphicon glyphicon-hdd"></span>&nbsp;Login</asp:LinkButton></li>
                    </ul>
                    <!--end nav-->

