        <div class="container">
            <asp:ContentPlaceHolder ID="MainTemplate" runat="server" />
        </div>

        <div class="container">

            <asp:MultiView ID="MultiView1" runat="server" ActiveViewIndex="0">

                <asp:View ID="View1" runat="server">
                    <div class="container">
                    </div>
                </asp:View>

                <asp:View ID="loginPage" runat="server">

                    <div class="container">
                        <div class="defaultviews">
                            <div class="input-group" style="width: 240px;">
                                <asp:TextBox class="form-control" placeholder="UserName" aria-describedby="basic-addon1" runat="server" ID="loginUserName"></asp:TextBox>

                            </div>
                            <div class="input-group" style="width: 240px; padding-top: 0.5em;">
                                <asp:TextBox class="form-control" TextMode="Password" placeholder="Password" aria-describedby="basic-addon1" runat="server" ID="loginPassword"></asp:TextBox>

                            </div>
                            <div class="input-group">
                                <div class="input-group" style="float: left; padding-top: 0.5em;">
                                    <asp:CheckBox ID="CheckBox1" runat="server" />

                                </div>
                                <div class="input-group" style="float: right; padding-top: 0.5em;">
                                    Keep me logged in
                                </div>
                            </div>
                            <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                                <div class="btn-group" role="group" aria-label="..." style="text-align: center;">
                                    <asp:Button ID="Button1" class="btn btn-default" runat="server" Text="Login" OnClick="Login" />
                                </div>
                            </div>
                            <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                                <asp:LinkButton ID="Button2" class="btn btn-default" runat="server" OnClick="ForgotPassword" Text="Forgot your password?" />

                            </div>
                            <div class="input-group">
                                <asp:Label runat="server" ID="loginResult"></asp:Label>
                            </div>
                        </div>
                    </div>
                </asp:View>

                <%--  Login Page End--%>

                <%--  SignUp Page Start--%>
                <asp:View ID="signUpPage" runat="server">

                    <div class="container">

                        <div class="container defaultviews" style="position: relative">
                            <h1>Sign Up Form</h1>
                            <div class="input-group" style="width: 361px; padding-top: 0.5em;">
                                <asp:TextBox class="form-control" placeholder="UserName" aria-describedby="basic-addon1" runat="server" ID="signUpUsername"></asp:TextBox>
                                <asp:Label ID="usernameerror" runat="server" Text=""></asp:Label>
                            </div>
                            <div class="input-group">
                                <div class="input-group" style="float: left; padding-top: 0.5em; padding-right: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="Password" TextMode="Password" aria-describedby="basic-addon1" runat="server" ID="signUpPassword">
                                    </asp:TextBox>
                                </div>
                                <div class="input-group " style="float: right; padding-top: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="Confirm Password" aria-describedby="basic-addon1" TextMode="Password" runat="server" ID="signUpPassword2"></asp:TextBox>
                                </div>
                            </div>
                            <div class="input-group">
                                <div class="input-group" style="float: left; padding-top: 0.5em; padding-right: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="First Name" aria-describedby="basic-addon1" runat="server" ID="fname">
                                    </asp:TextBox>
                                </div>
                                <div class="input-group" style="float: right; padding-top: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="Last Name" aria-describedby="basic-addon1" runat="server" ID="lname"></asp:TextBox>
                                </div>
                            </div>
                            <div class="input-group">
                                <div class="input-group" style="float: left; padding-top: 0.5em; padding-right: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="Security Question" aria-describedby="basic-addon1" runat="server" ID="secques">
                                    </asp:TextBox>
                                </div>
                                <div class="input-group" style="float: right; padding-top: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="Security Answer" aria-describedby="basic-addon1" runat="server" ID="secans"></asp:TextBox>
                                </div>
                            </div>
                            <div class="input-group">
                                <div class="input-group" style="float: left; padding-top: 0.5em; padding-right: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="Phone Number" aria-describedby="basic-addon1" runat="server" ID="phoneNumber">
                                    </asp:TextBox>
                                </div>
                                <div class="dropdown" style="float: right; width: 177px; padding-top: 0.5em;">
                                    <asp:DropDownList ID="ntwrkprovider" CssClass="form-control" runat="server">
                                        <asp:ListItem Selected="True">Provider</asp:ListItem>
                                        <asp:ListItem>boost mobile</asp:ListItem>
                                        <asp:ListItem>t-mobile</asp:ListItem>
                                        <asp:ListItem>virgin mobile</asp:ListItem>
                                        <asp:ListItem>cingular</asp:ListItem>
                                        <asp:ListItem>sprint nextel</asp:ListItem>
                                        <asp:ListItem>verizon</asp:ListItem>
                                        <asp:ListItem>nextel</asp:ListItem>
                                        <asp:ListItem>us cellular</asp:ListItem>
                                        <asp:ListItem>suncom</asp:ListItem>
                                        <asp:ListItem>powertel</asp:ListItem>
                                        <asp:ListItem>att cingular</asp:ListItem>
                                        <asp:ListItem>alltel</asp:ListItem>
                                        <asp:ListItem>metro pcs</asp:ListItem>
                                        <asp:ListItem></asp:ListItem>
                                    </asp:DropDownList>
                                </div>
                            </div>
                            <div class="input-group" style="text-align: center; padding-top: 0.5em;">
                                <asp:RadioButtonList ID="accType" runat="server" RepeatDirection="Horizontal">
                                    <asp:ListItem Text="Student" Value="S" />
                                    <asp:ListItem Text="Professor" Value="P" />
                                    <asp:ListItem Text="Viewer" Value="V" />
                                </asp:RadioButtonList>
                            </div>

                            <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                                <asp:Button Text="Submit" class="btn btn-default" ID="signUp" runat="server" OnClick="SignUp"></asp:Button>
                            </div>
                            <div>
                                <asp:Label ID="SignUpReply" ForeColor="Green" runat="server" Text=""></asp:Label>
                            </div>
                        </div>
                    </div>
                </asp:View>
                <%--  SignUp Page End--%>

                <%--  SignUp Page Start--%>
                <asp:View ID="forgotPasswordPage" runat="server">
                    <div class="container">
                        <div class="defaultviews">
                            <div class="defaultviews">
                                <div class="input-group" style="width: 240px; padding-top: 0.5em;">
                                    <asp:TextBox class="form-control" placeholder="UserName" aria-describedby="basic-addon1" runat="server" ID="forgotEmailId"></asp:TextBox>

                                </div>
                                <div class="defaultviews">
                                    <div class="input-group" style="width: 240px; padding-top: 0.5em;">
                                        <asp:RadioButtonList ID="FpAccntType" runat="server" RepeatDirection="Horizontal">
                                            <asp:ListItem Text="Student" Value="S" />
                                            <asp:ListItem Text="Professor" Value="P" />
                                            <asp:ListItem Text="Viewer" Value="V" />
                                        </asp:RadioButtonList>
                                    </div>
                                    <div class="input-group" style="width: 240px; padding-top: 0.5em;">
                                        <asp:RadioButtonList ID="retrievelMethodRadioList" runat="server" RepeatDirection="Horizontal">
                                            <asp:ListItem Text="Email" Value="E" />
                                            <asp:ListItem Text="Message" Value="M" />
                                        </asp:RadioButtonList>
                                    </div>
                                    <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                                        <asp:Button ID="Button3" class="btn btn-default" runat="server" Text="Retrieve Password" OnClick="RetrieveForgotPassword" />
                                    </div>

                                    <asp:Label ID="confirationMessage" ForeColor="Green" runat="server"></asp:Label>


                                </div>
                            </div>
                        </div>
                    </div>
                </asp:View>

                <asp:View ID="changePasswordMessage" runat="server">
                    <div class="defaultviews container">
                        <div class="input-group" style="width: 361px; padding-top: 0.5em;">
                            <asp:TextBox class="form-control" placeholder="Verification Code" EnableViewState="false" aria-describedby="basic-addon1" runat="server" ID="VerificationCode"></asp:TextBox>
                        </div>
                        <div class="input-group" style="width: 361px; padding-top: 0.5em;">
                            <asp:TextBox class="form-control" TextMode="Password" placeholder="New Password" EnableViewState="false" aria-describedby="basic-addon1" runat="server" ID="NewPassword"></asp:TextBox>
                        </div>
                        <div class="input-group" style="width: 361px; padding-top: 0.5em;">
                            <asp:TextBox class="form-control" TextMode="Password" placeholder="Confirm Password" EnableViewState="false" aria-describedby="basic-addon1" runat="server" ID="CnfrmPassword"></asp:TextBox>
                        </div>
                        <div class="btn-group" role="group" aria-label="..." style="text-align: center; padding-top: 0.5em;">
                            <asp:Button Text="Change Password" class="btn btn-default" ID="UpdatePass" runat="server" OnClick="UpdatePasswordUsingSms"></asp:Button>

                        </div> <br />
                        <asp:Label ID="UpdatePasswordFromSMSConfirmation" runat="server" Text=""></asp:Label>

                    </div>
                </asp:View>

                <%--  SignUp Page End--%>
            </asp:MultiView>
        </div>
